#define cfgRead(%1,%2,%3=nullstr) \
if instr(s,0,%1)!-1{	%c\
	i=0:p=0	%c\
	repeat	%c\
	p(2)=instr(s,p,"\"") : if p(2)=-1:break	%c\
	p+=p(2)	%c\
	p(1)=instr(s,p+1,"\"") : if p(1)=-1:break	%c\
	rtvaln(i)=strmid(s,p+1,p(1)):p+=strlen(rtvaln(i))+2	%c\
	i++		%c\
	loop	%c\
	%2:%3:continue}

