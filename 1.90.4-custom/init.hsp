		locvar_cnvdate_p -= 24 * 30 * 12
	}
	locvar_cnvdate_year = locvar_cnvdate_p / 24 / 30 / 12
	locvar_cnvdate_s = "" + locvar_cnvdate_year + lang("年", " ") + locvar_cnvdate_month + lang("月", "/") + locvar_cnvdate_day + lang("日", " ")
	if ( cnvdate_arg2 == 1 ) {
