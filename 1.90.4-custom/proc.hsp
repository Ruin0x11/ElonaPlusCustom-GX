		if ( ci >= RANGE_INV2 ) {
			cell_refresh inv(INV_ITEM_X, ci), inv(INV_ITEM_Y, ci)
		}
		else {
			if ( tc == CHARA_PLAYER ) {
				gosub *calcBurdenPc
			}
		}
		cdata(CDATA_THIRST, tc) += 2000
		cdata(CDATA_HUNGER, tc) += 150
	}
	if ( cdata(CDATA_HUNGER, tc) > 12000 * 2 ) {
		if ( rnd(5) == 0 ) {
			chara_vomit tc
		}
	}
