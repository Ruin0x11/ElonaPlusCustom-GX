		}
		if ( potionthrow != 90 ) {
			skillexp SKILL_NORMAL_ALCHEMY, cc, 40
		}
		else {
			skillexp SKILL_NORMAL_ALCHEMY, cc, 2
		}
	}
	if ( potionspill | potionthrow ) {
		potionspill = 0
		potionthrow = 0
		cdata(CDATA_THIRST, tc) += 400
