		}
		ninsin = 0
		return
	}
	if ( cbit(CHARA_BIT_PREGNANT, tc) == FALSE ) {
		txtef COLOR_YELLOW
		txt lang(name(tc) + "は寄生された。", name(tc) + " get" + _s(tc) + " pregnant.")
		if ( meal == 0 ) {
			animeload 8, tc
		}
		if ( ninsin != 0 ) {
			if ( ninsin == 674 ) {
				cdata(CDATA_INFANT, tc) = 674
			}
			if ( ninsin == 520 ) {
				cdata(CDATA_INFANT, tc) = 93
			}
			if ( ninsin == 422 ) {
				cdata(CDATA_INFANT, tc) = 798
			}
			if ( ninsin == 400 ) {
				cdata(CDATA_INFANT, tc) = 400
			}
		}
		else {
			cdata(CDATA_INFANT, tc) = 0
		}
		ninsin = 0
		cbitmod CHARA_BIT_PREGNANT, tc, TRUE
		if ( tc == CHARA_PLAYER ) {
			spact(SKILL_SPACT_PREGNANT - STARTING_SKILL_SPACT) = 1
		}
	}
	return



















































