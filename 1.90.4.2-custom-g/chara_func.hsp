	if ( cdata(CDATA_SP, actionsp_charid) < 50 ) {
		if ( cdata(CDATA_SP, actionsp_charid) < rnd(75) ) {
			dmgsp actionsp_charid, actionsp_arg2
			return 0
		}
	}
	dmgsp actionsp_charid, actionsp_arg2
	return 1

