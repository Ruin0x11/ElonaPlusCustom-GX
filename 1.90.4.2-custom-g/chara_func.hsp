#deffunc dmgmp int dmgmp_charid, int dmgmp_arg2
	cdata(CDATA_MP, dmgmp_charid) -= dmgmp_arg2
	if ( cdata(CDATA_MP, dmgmp_charid) < (-999999) ) {
