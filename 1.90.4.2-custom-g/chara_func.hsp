		if ( cdata(CDATA_ID, hostileaction_source) != CREATURE_ID_RAIZEL_THE_OLD_WIZARD | cdata(CDATA_ALLIED, hostileaction_source) != 0 | gdata(GDATA_FLAG_MAIN) < 760 ) {
