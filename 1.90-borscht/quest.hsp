			}
			qdata((10 /*!!!@[QDATA_ENCOUNTER @@@ QDATA_DEST_CLIENT]@!!! */), rq) = i
			flt
			dbmode = DBMODE_FIND
			flttypemajor = fsetdeliver(rnd(length(fsetdeliver)))
			gosub *db_item
			qdata(QDATA_PARAM1, rq) = flttypemajor
			qdata(QDATA_REWARD_ITEM, rq) = (5 /*!!!@[QUEST_REWARD_SUPPLY @@@ FILTER_RACE_KOBOLT]@!!! */)
			if ( flttypemajor == FILTER_ITEM_SPELLBOOK ) {
				qdata(QDATA_REWARD_ITEM, rq) = (2 /*!!!@[QUEST_REWARD_MAGIC @@@ FILTER_RACE_ORC]@!!! */)
			}
