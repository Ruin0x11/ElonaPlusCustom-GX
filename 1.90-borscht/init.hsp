			if ( gdata(GDATA_MONTH) == 4 ) {
				p = instr(buff, 0, "%ASISTER," + lang("JP", "EN"))
				break
			}
			else {
				p = instr(buff, 0, "%SISTER," + lang("JP", "EN"))
				break
			}
		}
		if ( cdata(CDATA_ID, tc) == CREATURE_ID_SHOPKEEPER_CROW ) {
