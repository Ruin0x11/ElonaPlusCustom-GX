*talk_quest_load
	notesel buffboard
	sdim buff2
	p = instr(buffboard, 0, s + "," + lang("JP", "EN"))
	buff2 = strmid(buffboard, p, instr(buffboard, p, "%END"))
	notesel buff2
	if ( noteinfo(0) <= 1 ) {
		buff2 = "no txt"
		return
	}
	p = rnd(noteinfo(0) - 1) + 1
	noteget buff2, p
	p = instr(buff2, 0, ":")
	s(3) = strmid(buff2, 0, p)
	if ( val == 2 ) {
		notesel buff
		return
	}
	buff2 = strmid(buff2, p + 1, strlen(buff2) - p - 1)
	if ( val != 2 ) {
		buff = buff2
	}
	return

