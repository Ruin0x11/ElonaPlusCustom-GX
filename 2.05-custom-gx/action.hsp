		if ( gdata(GDATA_MONTH) >= 6 & gdata(GDATA_MONTH) <= 8 ) {
			flttypeminor = FILTER_ITEM_ROD_DAMAGE
			if ( feat(FEAT_CELL_SUB_FEATURE_ID) == 10 + CELL_SUB_FEATURE_SEED_MAGIC ) {
				dbid = ITEM_ID_ROD_BUBBLE_BALL
			}
			if ( feat(FEAT_CELL_SUB_FEATURE_ID) == 20 + CELL_SUB_FEATURE_SEED_MAGIC ) {
				dbid = ITEM_ID_ROD_MAGIC_LASER
			}
			if ( feat(FEAT_CELL_SUB_FEATURE_ID) == 30 + CELL_SUB_FEATURE_SEED_MAGIC ) {
				dbid = ITEM_ID_ROD_POISON_BALL
			}
			if ( feat(FEAT_CELL_SUB_FEATURE_ID) == 40 + CELL_SUB_FEATURE_SEED_MAGIC ) {
				dbid = ITEM_ID_ROD_LIGHTNING_BALL
			}
			if ( feat(FEAT_CELL_SUB_FEATURE_ID) == 50 + CELL_SUB_FEATURE_SEED_MAGIC ) {
				dbid = ITEM_ID_ROD_ECLIPSE
			}
			if ( rnd(200) == 0 ) {
				dbid = ITEM_ID_ROD_DOMINATION
			}
		}
		if ( gdata(GDATA_MONTH) >= 9 & gdata(GDATA_MONTH) <= 11 ) {
			flttypeminor = FILTER_ITEM_ROD_UTILITY
			if ( feat(FEAT_CELL_SUB_FEATURE_ID) == 10 + CELL_SUB_FEATURE_SEED_MAGIC ) {
				dbid = ITEM_ID_ROD_WALL_CREATION
			}
			if ( feat(FEAT_CELL_SUB_FEATURE_ID) == 20 + CELL_SUB_FEATURE_SEED_MAGIC ) {
				dbid = ITEM_ID_ROD_ALCHEMY
			}
			if ( feat(FEAT_CELL_SUB_FEATURE_ID) == 30 + CELL_SUB_FEATURE_SEED_MAGIC ) {
				dbid = ITEM_ID_ROD_MAGIC_MAPPING
			}
			if ( feat(FEAT_CELL_SUB_FEATURE_ID) == 40 + CELL_SUB_FEATURE_SEED_MAGIC ) {
				dbid = ITEM_ID_ROD_TELEPORTATION
			}
			if ( feat(FEAT_CELL_SUB_FEATURE_ID) == 50 + CELL_SUB_FEATURE_SEED_MAGIC ) {
				dbid = ITEM_ID_ROD_IDENTIFY
			}
			if ( rnd(10000) == 0 ) {
				dbid = ITEM_ID_ROD_WISHING
			}
		}
		if ( gdata(GDATA_MONTH) == 12 | gdata(GDATA_MONTH) <= 2 ) {
			flttypeminor = FILTER_ITEM_ROD_ENVIRONMENTAL
			if ( feat(FEAT_CELL_SUB_FEATURE_ID) == 10 + CELL_SUB_FEATURE_SEED_MAGIC ) {
				dbid = ITEM_ID_ROD_CHANGE_CREATURE
			}
			if ( feat(FEAT_CELL_SUB_FEATURE_ID) == 20 + CELL_SUB_FEATURE_SEED_MAGIC ) {
				dbid = ITEM_ID_ROD_WEB
			}
			if ( feat(FEAT_CELL_SUB_FEATURE_ID) == 30 + CELL_SUB_FEATURE_SEED_MAGIC ) {
				dbid = ITEM_ID_ROD_SILENCE
			}
			if ( feat(FEAT_CELL_SUB_FEATURE_ID) == 40 + CELL_SUB_FEATURE_SEED_MAGIC ) {
				dbid = ITEM_ID_ROD_SLOW
			}
			if ( feat(FEAT_CELL_SUB_FEATURE_ID) == 50 + CELL_SUB_FEATURE_SEED_MAGIC ) {
				dbid = ITEM_ID_ROD_SUMMON_MONSTERS
			}
			if ( rnd(200) == 0 ) {
				dbid = ITEM_ID_ROD_DOMINATION
			}
		}
	}
	if ( gardenlv != 0 ) {
		if ( feat(FEAT_CELL_SUB_FEATURE_ID) > MAX_CELL_SUB_FEATURE ) {
			gardenlvlv = 3
		}
		gardenlvlv += limit(rnd((sdata(SKILL_NORMAL_GARDENING, CHARA_PLAYER) + 1) / 20 + 2), 0, 7)
		if ( feat == tile_plant + 6 ) {
			gardenlvlv += 5
		}

        if ( usecustom ) {
            itemcreate -1, dbid, cdata(CDATA_X, CHARA_PLAYER), cdata(CDATA_Y, CHARA_PLAYER), 0
        }
        else {
            itemcreate CHARA_PLAYER, dbid, -1, -1, 0
        }

		inv(INV_ITEM_STATUS, ci) = ITEM_STATUS_NORMAL
		inv(INV_ITEM_LEVEL, ci) += gardenlvlv
		gardenlv = 0
		gardenlvlv = 0
	}
	else {
        if ( usecustom ) {
            itemcreate -1, dbid, cdata(CDATA_X, CHARA_PLAYER), cdata(CDATA_Y, CHARA_PLAYER), 0
        }
        else {
            itemcreate CHARA_PLAYER, dbid, -1, -1, 0
        }

		inv(INV_ITEM_STATUS, ci) = ITEM_STATUS_NORMAL
	}

    if ( usecustom ) {
        item_stack -1, ci, 1
    }
    else {
	    txt lang(itemname(ci, 1) + "を収穫した。", "You harvest " + itemname(ci, 1) + ".")
	    item_stack CHARA_PLAYER, ci, 1
    }

	return

*act_pickpocket
	gdata(GDATA_ROW_ACT) = ACTION_STEAL
	gosub *generalAct
	return

