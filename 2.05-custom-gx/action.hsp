// Gender checks for evolution purposes.

#defcfunc evoismale int evoismale_charid
    locvar_newsex = cdatan(CDATAN_NEWSEX, evoismale_charid)

	switch locvar_newsex
        case ""
			if ( cdata(CDATA_SEX, evoismale_charid) == SEX_MALE ) {
				return TRUE
			}
    		swbreak
		case "男性"
		case "なし"
		case "自称男性"
		case "male"
		case "none"
		case "male?"
		case "trans-male"
			return TRUE
    		swbreak
		default
			swbreak
	swend

    return FALSE

#defcfunc evoisfemale int evoisfemale_charid
    locvar_newsex = cdatan(CDATAN_NEWSEX, evoisfemale_charid)

	switch locvar_newsex
        case ""
			if ( cdata(CDATA_SEX, evoisfemale_charid) == SEX_FEMALE ) {
				return TRUE
			}
    		swbreak
		case "女性"
		case "両性具有"
		case "自称女性"
		case "female"
		case "hermaphrodite"
		case "female?"
		case "trans-female"
			return TRUE
    		swbreak
		default
			swbreak
	swend

    return FALSE

