		}
		display_key wx + 58, wy + 66 + cnt * 19 - 2, cnt
	loop
	font lang(cfg_font1, cfg_font2), 14 - en * 2, 0
	cs_listbk
	repeat pagesize
		p = pagesize * page + cnt
		if ( p >= listmax ) {
			break
		}
		i = cdata(CDATA_PIC, list(0, p)) \ COLOR_TINT_MULT, cdata(CDATA_PIC, list(0, p)) / COLOR_TINT_MULT
		chara_preparepic i, i(1)
		pos wx + 40, wy + 74 + cnt * 19 - 8
