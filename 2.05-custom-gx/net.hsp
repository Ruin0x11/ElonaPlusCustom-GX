*net_chat
	if ( develop == 0 & chatdeny == 1 ) {
		if ( chatturn > 20 ) {
			chatdeny = 0
		}
		else {
			txt lang("もう少し待った方がいい気がする。", "You think you should wait a little more.")
			return
		}
