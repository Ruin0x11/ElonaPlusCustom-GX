	if ( cdata(CDATA_ID, cc) == CREATURE_ID_AIKAGE_THE_SHINOBI_MASK ) {
		if ( cdata(CDATA_PIC, cc) == xy2pic(0, 25) ) {
			cdata(CDATA_PIC, cc) = xy2pic(20, 4)
