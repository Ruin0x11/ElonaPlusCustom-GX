						anix = cdata(CDATA_X, cc)
						aniy = cdata(CDATA_Y, cc)
						gosub *anime
						cdata(CDATA_CONDITION_BIND, cc) = 999
						evadd EVENT_NAGAREW
						goto *turn_end
