			if ( cdata(CDATA_PIC, cc) == xy2pic(4, 4) | cdata(CDATA_PIC, cc) == xy2pic(21, 4) | cdata(CDATA_PIC, cc) == xy2pic(5, 4) | cdata(CDATA_PIC, cc) == xy2pic(23, 9) ) {
