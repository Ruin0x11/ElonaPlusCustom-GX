	if ( key == key_select(1) ) {
		goto *AIAbilityLearnMenu
	}
	if ( key == key_select(2) ) {
		goto *AIDeleteAbilityMenu
	}
	if ( key == key_cancel | key == key_select(3) ) {
		goto *AIMainMenu
	}
	goto *AITeachConfigMenu
