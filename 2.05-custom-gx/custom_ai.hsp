		ValueName = AITextData(AIData(55+SlotID, tc-1) \ 100, 4) + "%"
	}
	if ( AIData(55+SlotID, tc-1) >= 200 & AIData(55+SlotID, tc-1) < 300 ) {
		ValueName = AITextData(AIData(55+SlotID, tc-1) \ 200, 5)
	}
