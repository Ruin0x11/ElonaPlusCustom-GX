			}
		loop
	}
	else {
		repeat MAX_CHARA_TOTAL - MAX_CHARA_SAVE, MAX_CHARA_SAVE
			if ( cbit(CHARA_BIT_FESTIVAL, cnt) == 1 ) {
				chara_vanquish cnt
			}
		loop
	}
	return

