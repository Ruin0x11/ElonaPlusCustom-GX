	:pget 128,0\
	:pos 256,0\
	:gcopy BUFFER_PCC+prm_cc,128,0,128,198\
	:pos 256,0\
	:gfini@ 128,198\
	:gfdec2@ c_col@(0,%2/1000),c_col@(1,%2/1000),c_col@(2,%2/1000)\
