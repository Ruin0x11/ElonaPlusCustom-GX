#deffunc create_pcpic int prm_cc, int prm_show_equip
	buffer BUFFER_PCC + prm_cc, 384, 198
	color 0, 0, 0
	boxf
	if ( pcc(PCC_BODY, prm_cc) == 0 ) {
		pcc(PCC_BODY, prm_cc) = cdata(CDATA_SEX, prm_cc) + 1
		pcc(PCC_EYE, prm_cc) = cdata(CDATA_SEX, prm_cc) + 7
		pcc(PCC_HAIR, prm_cc) = 2 + rnd(MAX_COLOR) * COLOR_TINT_MULT
		pcc(PCC_CLOTH, prm_cc) = 1 + rnd(MAX_COLOR) * COLOR_TINT_MULT
		pcc(PCC_PANTS, prm_cc) = 1 + rnd(MAX_COLOR) * COLOR_TINT_MULT
		pcc(PCC_RIDE, prm_cc) = 1
	}
	if ( prm_show_equip == 1 ) {
		pcc(PCC_MANTLE, prm_cc) = 0
		pcc(PCC_CHEST, prm_cc) = 0
		pcc(PCC_BOOTS, prm_cc) = 0
		pcc(PCC_LEG, prm_cc) = 0
		pcc(PCC_GLOVE, prm_cc) = 0
		pcc(PCC_BELT, prm_cc) = 0
		repeat MAX_EQUIP_SLOT_TOTAL, CDATA_STARTING_EQUIP_SLOTS
			if ( cdata(cnt, prm_cc) \ EXT_EQUIP_SLOTS != 0 ) {
				locvar_create_pcpic_i = cdata(cnt, prm_cc) \ EXT_EQUIP_SLOTS - 1
				locvar_create_pcpic_body = iequiploc(locvar_create_pcpic_i)
				gosub *pcc_wear
			}
		loop
	}
	if ( gdata(GDATA_FLAG_WORLD_MAP_VEHICLE_KIND) > VEHICLE_NONE & mdata(MDATA_TYPE) == MAP_TYPE_WORLD ) {
		if ( prm_cc == CHARA_PLAYER ) {
            pccSet("world", gdata(GDATA_FLAG_WORLD_MAP_VEHICLE_PIC))
		}
	}
	else {
        gosub *create_pcpic_normal
    }

	gsel BUFFER_SCREEN

	return

*create_pcpic_normal
	; Copy the colors for main hair/body to sub hair/eyes.
	pcc(PCC_SUB_HAIR, prm_cc) = pcc(PCC_HAIR, prm_cc) / COLOR_TINT_MULT * COLOR_TINT_MULT + pcc(PCC_SUB_HAIR, prm_cc) \ COLOR_TINT_MULT
	pcc(PCC_EYE, prm_cc) = pcc(PCC_BODY, prm_cc) / COLOR_TINT_MULT * COLOR_TINT_MULT + pcc(PCC_EYE, prm_cc) \ COLOR_TINT_MULT

	if ( prm_show_equip != 0 ) {
		if ( pcc(PCC_HIDE_MANTLE, prm_cc) == 0 ) {
	        pccSet("mantle", pcc(PCC_MANTLE, prm_cc))
		}
	}
	pccSet("hairbk", pcc(PCC_HAIR, prm_cc))
	if ( prm_cc == 0 & gdata(GDATA_RIDER) != 0 & pcc(PCC_RIDE, prm_cc) != 0 ) {
	    pccSet("ridebk", pcc(PCC_RIDE, prm_cc))
	}
	else {
	    pccSet("body", pcc(PCC_BODY, prm_cc))
	}
	pccSet("eye", pcc(PCC_EYE, prm_cc))
	if ( prm_cc != 0 | gdata(GDATA_RIDER) == 0 | pcc(PCC_RIDE, prm_cc) == 0 ) {
	    pccSet("pants", pcc(PCC_PANTS, prm_cc))
	}
	pccSet("cloth", pcc(PCC_CLOTH, prm_cc))
	if ( prm_show_equip != 0 ) {
		if ( pcc(PCC_HIDE_CHEST, prm_cc) == 0 ) {
	        pccSet("chest", pcc(PCC_CHEST, prm_cc))
		}
		if ( prm_cc != 0 | gdata(GDATA_RIDER) == 0 | pcc(PCC_RIDE, prm_cc) == 0 ) {
			if ( pcc(PCC_HIDE_LEG, prm_cc) == 0 ) {
	            pccSet("leg", pcc(PCC_LEG, prm_cc))
			}
		}
		if ( pcc(PCC_HIDE_BELT, prm_cc) == 0 ) {
	        pccSet("belt", pcc(PCC_BELT, prm_cc))
		}
		if ( pcc(PCC_HIDE_GLOVE, prm_cc) == 0 ) {
	        pccSet("glove", pcc(PCC_GLOVE, prm_cc))
		}
	}
	if ( prm_cc == 0 ) {
		if ( gdata(GDATA_RIDER) != 0 ) {
	        pccSet("ride", pcc(PCC_RIDE, prm_cc))
		}
	}
	if ( prm_show_equip != 0 ) {
		if ( pcc(PCC_HIDE_MANTLE, prm_cc) == 0 ) {
	        pccSet("mantlebk", pcc(PCC_MANTLE, prm_cc))
		}
	}
	pccSet("hair", pcc(PCC_HAIR, prm_cc))
	pccSet("subhair", pcc(PCC_SUB_HAIR, prm_cc))
	pccSet("etc", pcc(PCC_ETC1, prm_cc))
	pccSet("etc", pcc(PCC_ETC2, prm_cc))
	pccSet("etc", pcc(PCC_ETC3, prm_cc))

    return

*pcc_wear
	locvar_create_pcpic_p = refitem(inv(INV_ITEM_ID, locvar_create_pcpic_i), DBSPEC_PCC)
	if ( locvar_create_pcpic_p == 0 ) {
		locvar_create_pcpic_p = 1
	}
	if ( locvar_create_pcpic_body == EQUIP_SLOT_HEAD ) {
		return
	}
	if ( locvar_create_pcpic_body == EQUIP_SLOT_BACK ) {
		pcc(PCC_MANTLE, prm_cc) = locvar_create_pcpic_p + inv(INV_ITEM_COL, locvar_create_pcpic_i) * COLOR_TINT_MULT
		return
	}
	if ( locvar_create_pcpic_body == EQUIP_SLOT_BODY ) {
		pcc(PCC_CHEST, prm_cc) = locvar_create_pcpic_p + inv(INV_ITEM_COL, locvar_create_pcpic_i) * COLOR_TINT_MULT
		return
	}
	if ( locvar_create_pcpic_body == EQUIP_SLOT_LEG ) {
		pcc(PCC_LEG, prm_cc) = locvar_create_pcpic_p + inv(INV_ITEM_COL, locvar_create_pcpic_i) * COLOR_TINT_MULT
		return
	}
	if ( locvar_create_pcpic_body == EQUIP_SLOT_ARM ) {
		pcc(PCC_GLOVE, prm_cc) = locvar_create_pcpic_p + inv(INV_ITEM_COL, locvar_create_pcpic_i) * COLOR_TINT_MULT
		return
	}
	if ( locvar_create_pcpic_body == EQUIP_SLOT_WAIST ) {
		pcc(PCC_BELT, prm_cc) = locvar_create_pcpic_p + inv(INV_ITEM_COL, locvar_create_pcpic_i) * COLOR_TINT_MULT
		return
	}
	return

