		medit = 1
		goto *mapEditor_init
	}

    if ( dbg_quickStart ) {
	    mode = MODELIST_QUICK_START
	    goto *world_init
    }

	if ( defload != "" ) {
		exist exedir + "save\\" + defload + "\\header.txt"
		if ( strsize == (-1) ) {
			exist exedir + "save\\sav_" + defload + "\\header.txt"
			if ( strsize != (-1) ) {
				defload = "sav_" + defload
			}
			else {
				defload = ""
			}
		}
		if ( defload == "" ) {
			dialog "Invalid defLoadFolder. name"
		}
		else {
			playerid = defload
			mode = MODELIST_CONTINUE
			music = MUSICLIST_NONE
			goto *world_init
		}
	}

